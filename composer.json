{
  "name": "garethellis36/ivcc-site",
  "description": "Iffley Village Cricket Club website",
  "homepage": "https://ivcc.co.uk",
  "type": "project",
  "license": "Proprietary",
  "require": {
    "php": ">=5.4.16",
    "cakephp/cakephp": "^3.3",
    "mobiledetect/mobiledetectlib": "2.*",
    "cakephp/plugin-installer": "*",
    "symfony/yaml": "^2.8",
    "pgbi/cakephp3-soft-delete": "^1.3",
    "intervention/image": "^2.3",
    "garethellis/cricket-stats-helper": "^1.0",
    "composer/installers": "^1.1",
    "cakephp/migrations": "1.6.3"
  },
  "require-dev": {
    "psy/psysh": "@stable",
    "cakephp/debug_kit": "~3.0",
    "cakephp/bake": "~1.0",
    "phpunit/phpunit": "^5.5"
  },
  "autoload": {
    "psr-4": {
      "App\\": "src"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "App\\Test\\": "tests",
      "Cake\\Test\\": "./vendor/cakephp/cakephp/tests"
    }
  },
  "scripts": {
    "post-autoload-dump": "Cake\\Composer\\Installer\\PluginInstaller::postAutoloadDump"
  },
  "minimum-stability": "dev",
  "prefer-stable": true
}
